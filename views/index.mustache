<div id="modules-container">
  <div class="scroll-container">
    <div class="title">
      <span>Your Modules</span>
    </div>
    <ul id="modules">
    {{#modules}}
      <li>
        <div class="padding">
          <span class="name">{{ name }}</span>
          <span class="version">{{ version }}</span>
          <p>{{ description }}</p>
          <a class="repository" target="_blank" href="{{ homepage }}">docs &amp; source</a>
          <script> {{{ source }}} </script>
          <script> introspect.diff("{{ name }}"); </script>
        </div>
      </li>
    {{/modules}}
    </ul>
    <script> introspect.cull() </script>
    <div id="add">
      Add a module to your ender
    </div>
  </div>
</div>
<div id="functions-container">
  <script type="text/ender-template" id="template">
    <li>
      <span class="<%= extraClass %>">
        <em class="money"><%= prefix %></em>
        <strong><%= name %> <%= aliases %></strong>
      </span>
      <div class="info">
        <div class="docs">
          <span class="owner"><%= owner %></span>
        </div>
        <pre><code><%= func %></code></pre>
      </div>
    </li>
  </script>
  <div id="searchbox">
    <input type="text" id="livesearch" value="Search yer functions"/>
  </div>
  <div class="title">
    <span>Your Functions</span>
    <span class="directory">{{ directory }}</span>
  </div>
  <div id="function-scroller">
    <ul id="functions">
    </ul>
  </div>
</div>